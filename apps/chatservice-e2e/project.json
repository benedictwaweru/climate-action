{
  "name": "chatservice-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["chatservice"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/chatservice-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["chatservice:build", "chatservice:serve"]
    }
  }
}
