{
  "name": "dashboardservice-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["dashboardservice"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/dashboardservice-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["dashboardservice:build", "dashboardservice:serve"]
    }
  }
}
